      <TableColumn fx:id="instructionColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="266.0" sortable="false" text="Instruction" />
      <TableColumn fx:id="cyclesColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="30.0" prefWidth="109.0" sortable="false" text="Cycles" />
  </columns>
   <columnResizePolicy>
      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
