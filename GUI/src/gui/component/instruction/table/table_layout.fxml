<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="instructionTable" style="-fx-alignment: CENTER;" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.component.instruction.table.InstructionTableController">
  <columns>
    <TableColumn fx:id="lineNumberColumn" maxWidth="1.7976931348623157E308" minWidth="40.0" prefWidth="75.0" sortable="false" text="Line" />
    <TableColumn fx:id="labelColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="40.0" prefWidth="75.0" sortable="false" text="Label" />
