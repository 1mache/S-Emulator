<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="rght" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="History" text="History">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Button fx:id="show" mnemonicParsing="false" onAction="#showHistoryListener" text="Show" />
            <Button fx:id="reRun" mnemonicParsing="false" onAction="#reRunListener" text="Re-run" />
         </children>
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
      <TableView fx:id="historyTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <columns>
            <TableColumn fx:id="Index" editable="false" prefWidth="58.666624903678894" style="-fx-alignment: CENTER;" text="Index" />
            <TableColumn fx:id="level" editable="false" prefWidth="47.333309173583984" style="-fx-alignment: CENTER;" text="level" />
            <TableColumn fx:id="inputs" editable="false" prefWidth="165.99998474121094" style="-fx-alignment: CENTER;" text="inputs" />
            <TableColumn fx:id="output" editable="false" prefWidth="56.66668701171875" style="-fx-alignment: CENTER;" text="output" />
            <TableColumn fx:id="cycles" editable="false" prefWidth="50.00006103515625" style="-fx-alignment: CENTER;" text="cycels" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets left="5.0" right="5.0" />
   </padding>
</VBox>
